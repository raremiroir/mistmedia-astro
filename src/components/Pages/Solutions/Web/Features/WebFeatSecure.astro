---
import { t } from 'i18next';
import { Picture } from "@astrojs/image/components";
import ContentWrap from "@/components/Pages/ContentWrap.astro";
import Link from "@/components/Common/Button/Link.svelte";

let text: string[] = [];
Object.values(t('services:solutions.web.features.secure.content.text')).map((value: string) => {
   text.push(value);
});
---

<ContentWrap >
   <section>
      { text.map((textItem) => ( <p set:html={textItem} />)) }
   </section>
</ContentWrap>